arxivId = {1110.5223},
author = {Morcos, F. and Pagnani, A. and Lunt, B. and Bertolino, A. and Marks, D. S. and Sander, C. and Zecchina, R. and Onuchic, J. N. and Hwa, T. and Weigt, M.},
doi = {10.1073/pnas.1111471108},
eprint = {1110.5223},
file = {:baycells/home/fallain/Dropbox/These/doc/Docear/literature_repository/2011 - Direct-coupling analysis of residue coevolution captures native contacts across many protein families.pdf:pdf;:baycells/home/fallain/Dropbox/These/doc/Docear/literature_repository/2011 - Direct-coupling analysis of residue coevolution captures native contacts across many protein families(2).pdf:pdf},
isbn = {1091-6490 (Electronic)\r0027-8424 (Linking)},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
month = {dec},
