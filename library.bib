	pmcid = {PMC2606947},
	keywords = {Acetylation, Acetylcysteine, Animals, Cell Line, Cystic Fibrosis, Cytokines, Epithelial Cells, Free Radical Scavengers, Humans, Hydrogen Peroxide, Interleukin-8, Oxidants, Oxidative Stress, Promoter Regions, Genetic, Respiratory Mucosa, Transcription, Genetic}
