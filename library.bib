editor = {Sali, Andrej},
eprint = {1110.5091},
file = {:baycells/home/fallain/Dropbox/These/doc/Docear/literature_repository/2011 - Protein 3D structure computed from evolutionary sequence variation.pdf:pdf;:baycells/home/fallain/Dropbox/These/doc/Docear/literature_repository/2011 - Protein 3D structure computed from evolutionary sequence variation(2).pdf:pdf},
isbn = {1932-6203},
issn = {19326203},
journal = {PLoS ONE},
month = {dec},
number = {12},
pages = {e28766},
pmid = {22163331},
title = {{Protein 3D structure computed from evolutionary sequence variation}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22163331 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3233603 http://dx.plos.org/10.1371/journal.pone.0028766},
