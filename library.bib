pmid = {9218963},
title = {{Improving contact predictions by the combination of correlated mutations and other sources of sequence information}},
url = {http://www.sciencedirect.com/science/article/pii/S1359027897000606 http://www.sciencedirect.com/science/article/pii/S1359027897000606%5Cnhttp://www.sciencedirect.com.ezproxy.lib.monash.edu.au/science/article/pii/S1359027897000606%5Cnhttp://www.sciencedire},
