@article{jiang_oxidant-induced_2005,
	title = {Oxidant-induced apoptosis in human retinal pigment epithelial cells: dependence on extracellular redox state},
	volume = {46},
	issn = {0146-0404},
	doi = {10.1167/iovs.04-0949},
	shorttitle = {Oxidant-induced apoptosis in human retinal pigment epithelial cells},
	abstract = {{PURPOSE}: To test whether variation in extracellular cysteine (Cys) redox potential (E(h)) over the physiologic range occurring in human plasma affects oxidant-induced apoptosis in cultured human retinal pigment epithelial ({hRPE}) cells.
{METHODS}: The {hRPE} cells were incubated in culture medium with E(h) established over the range of -16 {mV} (most oxidized) to -158 {mV} (most reduced) by adding different concentrations of Cys and cystine ({CySS}) with constant total Cys equivalents. Apoptosis was induced with tert-butylhydroperoxide ({tBH}).
{RESULTS}: The {hRPE} cells were sensitized to {tBH}-induced apoptosis in the more oxidized extracellular conditions (E(h) {\textgreater} -55 {mV}) compared with the reduced conditions (E(h) {\textless} -89 {mV}). Loss of mitochondrial membrane potential (Deltapsi(m)), release of cytochrome c, and activation of caspase 3 after {tBH} treatments all increased under the more oxidized conditions. However, the extracellular redox state did not affect expression of Fas or {FasL} in {hRPE} cells.
{CONCLUSIONS}: The {hRPE} cells that are exposed to a more oxidized extracellular redox environment have increased susceptibility to oxidant-induced apoptosis through the intrinsic mitochondrial pathway, which could contribute to an age-related decline in cell populations in the retina and thereby provide a potential mechanism for the degenerative changes that are associated with age-related macular degeneration ({ARMD}).},
	pages = {1054--1061},
	number = {3},
	journaltitle = {Investigative Ophthalmology \& Visual Science},
	shortjournal = {Invest. Ophthalmol. Vis. Sci.},
	author = {Jiang, Shunai and Moriarty-Craige, Siobhan E. and Orr, Michael and Cai, Jiyang and Sternberg, Paul and Jones, Dean P.},
	date = {2005-03},
	pmid = {15728565},
	keywords = {Adult, Aged, Antigens, {CD}95, Apoptosis, Blotting, Western, Caspase 3, Caspases, Cells, Cultured, Cysteine, Cystine, Cytochromes c, Extracellular Space, Fas Ligand Protein, Humans, In Situ Nick-End Labeling, Membrane Glycoproteins, Membrane Potentials, Mitochondria, Oxidation-Reduction, Pigment Epithelium of Eye, tert-Butylhydroperoxide}
