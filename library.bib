@article{biemans-oldehinkel_abc_2006,
	title = {{ABC} transporter architecture and regulatory roles of accessory domains},
	volume = {580},
	issn = {0014-5793},
	doi = {10.1016/j.febslet.2005.11.079},
	abstract = {We present an overview of the architecture of {ATP}-binding cassette ({ABC}) transporters and dissect the systems in core and accessory domains. The {ABC} transporter core is formed by the transmembrane domains ({TMDs}) and the nucleotide binding domains ({NBDs}) that constitute the actual translocator. The accessory domains include the substrate-binding proteins, that function as high affinity receptors in {ABC} type uptake systems, and regulatory or catalytic domains that can be fused to either the {TMDs} or {NBDs}. The regulatory domains add unique functions to the transporters allowing the systems to act as channel conductance regulators, osmosensors/regulators, and assemble into macromolecular complexes with specific properties.},
	pages = {1023--1035},
	number = {4},
	journaltitle = {{FEBS} letters},
	shortjournal = {{FEBS} Lett.},
	author = {Biemans-Oldehinkel, Esther and Doeven, Mark K. and Poolman, Bert},
	date = {2006-02-13},
	pmid = {16375896},
	keywords = {{ATP}-Binding Cassette Transporters, {ATP}-Binding Cassette Transporters, Cell Membrane, Cell Membrane, Protein Conformation, Protein Conformation}
}

@article{dawson_cftr:_1999,
	title = {{CFTR}: mechanism of anion conduction},
	volume = {79},
	issn = {0031-9333},
	shorttitle = {{CFTR}},
	abstract = {{CFTR}: Mechanism of Anion Conduction. Physiol. Rev. 79, Suppl.: S47-S75, 1999. - The purpose of this review is to collect together the results of recent investigations of anion conductance by the cystic fibrosis transmembrane conductance regulator along with some of the basic background that is a prerequisite for developing some physical picture of the conduction process. The review begins with an introduction to the concepts of permeability and conductance and the Nernst-Planck and rate theory models that are used to interpret these parameters. Some of the physical forces that impinge on anion conductance are considered in the context of permeability selectivity and anion binding to proteins. Probes of the conduction process are considered, particularly permeant anions that bind tightly within the pore and block anion flow. Finally, structure-function studies are reviewed in the context of some predictions for the origin of pore properties.},
	pages = {S47--75},
	number = {1},
	journaltitle = {Physiological Reviews},
	shortjournal = {Physiol. Rev.},
	author = {Dawson, D. C. and Smith, S. S. and Mansoura, M. K.},
	date = {1999-01},
	pmid = {9922376},
	keywords = {Amino Acid Sequence, Amino Acid Sequence, Animals, Animals, Anions, Anions, Biological, Cell Membrane Permeability, Cell Membrane Permeability, Cystic Fibrosis Transmembrane Conductance Regulator, Cystic Fibrosis Transmembrane Conductance Regulator, Humans, Humans, Membrane Potentials, Membrane Potentials, Models, Models, Biological, Molecular Probes, Molecular Probes, Molecular Sequence Data, Molecular Sequence Data, Structure-Activity Relationship, Structure-Activity Relationship, Substrate Specificity, Substrate Specificity}
