journal = {Current Protein & Peptide Science},
keywords = {Algorithms,Amino Acid Sequence,Databases,Models,Molecular,Molecular Sequence Data,Multiprotein Complexes,Protein,Protein Binding,Protein Conformation,Protein Interaction Mapping,Proteins,review},
