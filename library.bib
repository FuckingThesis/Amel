title = {{ARIA: Automated NOE assignment and NMR structure calculation}},
url = {http://bioinformatics.oxfordjournals.org/content/19/2/315 http://www.ncbi.nlm.nih.gov/pubmed/12538267 internal-pdf://0.0.0.240/315.html},
