}

@Article{pillarisetti_infection_2011,
  Title                    = {Infection, inflammation, and lung function decline in infants with cystic fibrosis},
  Author                   = {Pillarisetti, Naveen and Williamson, Elizabeth and Linnane, Barry and Skoric, Billy and Robertson, Colin F. and Robinson, Phil and Massie, John and Hall, Graham L. and Sly, Peter and Stick, Stephen and Ranganathan, Sarath and {Australian Respiratory Early Surveillance Team for Cystic Fibrosis (AREST CF)}},
  Number                   = {1},
  Pages                    = {75--81},
  Volume                   = {184},

  Abstract                 = {{RATIONALE}: Better understanding of evolution of lung function in infants with cystic fibrosis ({CF}) and its association with pulmonary inflammation and infection is crucial in informing both early intervention studies aimed at limiting lung damage and the role of lung function as outcomes in such studies.
{OBJECTIVES}: To describe longitudinal change in lung function in infants with {CF} and its association with pulmonary infection and inflammation.
{METHODS}: Infants diagnosed after newborn screening or clinical presentation were recruited prospectively. {FVC}, forced expiratory volume in 0.5 seconds ({FEV}(0.5)), and forced expiratory flows at 75\% of exhaled vital capacity ({FEF}(75)) were measured using the raised-volume technique, and z-scores were calculated from published reference equations. Pulmonary infection and inflammation were measured in bronchoalveolar lavage within 48 hours of lung function testing.
{MEASUREMENTS} {AND} {MAIN} {RESULTS}: Thirty-seven infants had at least two successful repeat lung function measurements. Mean ({SD}) z-scores for {FVC} were -0.8 (1.0), -0.9 (1.1), and -1.7 (1.2) when measured at the first visit, 1-year visit, or 2-year visit, respectively. Mean ({SD}) z-scores for {FEV}(0.5) were -1.4 (1.2), -2.4 (1.1), and -4.3 (1.6), respectively. In those infants in whom free neutrophil elastase was detected, {FVC} z-scores were 0.81 lower (P=0.003), and {FEV}(0.5) z-scores 0.96 lower (P=0.001), respectively. Significantly greater decline in {FEV}(0.5) z-scores occurred in those infected with Staphylococcus aureus (P=0.018) or Pseudomonas aeruginosa (P=0.021).
{CONCLUSIONS}: In infants with {CF}, pulmonary inflammation is associated with lower lung function, whereas pulmonary infection is associated with a greater rate of decline in lung function. Strategies targeting pulmonary inflammation and infection are required to prevent early decline in lung function in infants with {CF}.},
  Date                     = {2011-07-01},
  Doi                      = {10.1164/rccm.201011-1892OC},
  ISSN                     = {1535-4970},
  Journaltitle             = {American Journal of Respiratory and Critical Care Medicine},
  Keywords                 = {Bronchoalveolar Lavage Fluid, Bronchoalveolar Lavage Fluid, Cystic Fibrosis, Cystic Fibrosis, Disease Progression, Disease Progression, Female, Female, Forced Expiratory Volume, Forced Expiratory Volume, Humans, Humans, Infant, Infant, Infant, Newborn, Male, Male, Neonatal Screening, Neonatal Screening, Newborn, Pneumonia, Pneumonia, Pseudomonas Infections, Pseudomonas Infections, Respiratory Function Tests, Respiratory Function Tests, Respiratory Tract Infections, Respiratory Tract Infections, Staphylococcal Infections, Staphylococcal Infections, Vital Capacity, Vital Capacity},
  Pmid                     = {21493738},
  Shortjournal             = {Am. J. Respir. Crit. Care Med.}
}

