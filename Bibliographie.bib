@Article{schwiebert_cftr_1999,
  Title                    = {{CFTR} is a conductance regulator as well as a chloride channel},
  Author                   = {Schwiebert, E. M. and Benos, D. J. and Egan, M. E. and Stutts, M. J. and Guggino, W. B.},
